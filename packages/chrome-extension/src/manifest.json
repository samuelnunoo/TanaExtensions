{
  "manifest_version": 3,
  "name": "Tana Extension",
  "version": "0.1.0",
  "description": "An extension to modify Tana.",
  "content_scripts": [
    {
      "js": [
        "lib/ResourceInjector.ts"
      ],
      "matches": [
        "https://app.tana.inc/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/*","lib/index.ts","content/extensions.ts"],
      "matches": ["https://app.tana.inc/*"]
    }
  ]
}